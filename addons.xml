<addons>
<addon id="plugin.audio.baiduradio"
    name="BaiduRadio"
    version="1.0.0"
    provider-name="Robinttt">
    <extension
        point="xbmc.python.pluginsource"
        library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>BaiduRadio audio plugin</summary>
        <summary lang="zh">百度电台联盟音频插件</summary>
        <description>BaiduRadio audio plugin</description>
        <description lang="zh">百度电台联盟音频插件，在线收听功能受电台播出时间和网络条件限制，个别台可能无法收听。</description>
        <platform>all</platform>
    </extension>
</addon>

